@model IEnumerable<WebBanXeMay.Models.XeMay>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenXe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HangSX)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgaySX)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaiXe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoTaXe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HinhAnh)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenXe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HangSX)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgaySX)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoaiXe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoTaXe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HinhAnh)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDXe">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDXe">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDXe">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
